{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "UserDbConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=DemoLoanProject;Integrated Security=True"
  },


  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "ConnectionStrings": {
            "ConnectionString": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=SeriLogDb;Integrated Security=True"
          },
          "tableName": "Logs",
          "autoCreatedSqlTable": true
        }
      }
    ]
  },
  "Access-Control-Allow-Origin": "*",
  "Access-Control-Allow-Methods": "GET,POST,OPTIONS,DELETE,PUT"
}
